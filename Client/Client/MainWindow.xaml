<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
		<StackPanel>
			<TextBox Name="txtUserName" />
			<TextBox Name="txtPassword" />
			<Button Name="btnListGames" Click="btnListGames_Click">ListGames</Button>
			<TextBox Name="txtGameID" />
			<Button Name="btnJoin" Click="btnJoin_Click">Join</Button>
			<Button Name="btnStartGame" Click="btnStartGame_Click">Start</Button>
			<TextBox Name="txtInput"></TextBox>
			<TextBox Name="txtInput2"></TextBox>
			<!--<Button Name=""-->
			<TextBox Name="blkLog" Height="100" TextWrapping="Wrap"></TextBox>
		</StackPanel>
        
    </Grid>
</Window>
